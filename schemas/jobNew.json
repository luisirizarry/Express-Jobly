{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Software Engineer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "minimum": 0,
            "title": "The salary Schema",
            "examples": [
                120000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "minimum": 0,
            "maximum": 1,
            "title": "The equity Schema",
            "examples": [
                0.05
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "tech-corp"
            ]
        }
    },
    "examples": [{
        "title": "Software Engineer",
        "salary": 120000,
        "equity": 0.05,
        "companyHandle": "tech-corp"
    }]
}